<Page x:Class="traqpaqWPF.DataPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="451"
	Title="DataPage">
    <Grid Name="mainGrid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100*" />
            <ColumnDefinition Width="200*" />
        </Grid.ColumnDefinitions>
        <Grid Name="subGrid">
            <Grid.RowDefinitions>
                <RowDefinition Height="200*" />
                <RowDefinition Height="150*" />
            </Grid.RowDefinitions>
            <GridSplitter Height="3" Name="gridSplitter2" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" Grid.Row="0" />
        </Grid>
        <GridSplitter Grid.Column="0" Name="gridSplitter1" HorizontalAlignment="Right" Width="3"/>
    </Grid>
</Page>
